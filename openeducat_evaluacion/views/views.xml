<?xml version="1.0" encoding="utf-8"?>
<odoo>


  <template id="portal_mydetails_home_inherit" inherit_id="portal.portal_layout">
      <xpath expr="//div[hasclass('o_portal_my_details')]" position="inside">
          <p>Genero: <span t-field="user_id.gender"/></p>
          <p>Fecha de Nacimiento:  <span t-field="user_id.user_line.birth_date"/> </p>
          <p>Visa Info: <span t-field="user_id.user_line.visa_info"/> </p>
          <div t-field="user_id.user_line.blood_group"/>
          <div t-field="user_id.user_line.emergency_contact"/>
          <div t-field="user_id.user_line.phone"/>
          <div t-field="user_id.user_line.email"/>
          <div t-field="user_id.user_line.enrollment_number"/>
          <div t-field="user_id.user_line.centro_trabajo"/>
          <div t-field="user_id.user_line.rfc"/>
          <div t-field="user_id.user_line.registration_date"/>
          <div t-field="user_id.user_line.nationality"/>
          <div t-field="user_id.user_line.category_id.name"/>
          <div t-field="user_id.user_line.mobile"/>
          <div t-field="user_id.user_line.user_id"/>
          <div t-field="user_id.user_line.institutional_email"/>
          <div t-field="user_id.user_line.vigencia_credencial"/>
          <div t-field="user_id.user_line.curp"/>

      </xpath>
  </template>
</odoo>