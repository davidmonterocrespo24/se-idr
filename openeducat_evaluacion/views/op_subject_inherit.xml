        <!-- Inherit Form View to Modify it -->
        <record id="op_student_form_inherit" model="ir.ui.view">
            <field name="name">formopstudent</field>
            <field name="model">op.subject</field>
            <field name="inherit_id" ref="openeducat_core.view_op_subject_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='subject_type']" position="after">
                    <field name="nota"/>
                </xpath>

            </field>
        </record>

        <record id="op_student_tree_inherit" model="ir.ui.view">
            <field name="name">treeopstudent</field>
            <field name="model">op.subject</field>
            <field name="inherit_id" ref="openeducat_core.view_op_subject_tree"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='subject_type']" position="after">
                    <field name="nota"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>